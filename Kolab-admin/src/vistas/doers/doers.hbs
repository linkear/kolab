<link rel="stylesheet" href="">
<form action="/doers/detalle/{{user.idUsuarios}}" method="get"></form>
<div id="contenido">
    {{#each doers}}
    <div id="cedula">
        <p>{{Cedula}}</p>
    </div>
    <div id="Nombres">
        <label>{{NombreDoers}}</label>
    </div>
    <div id="Edad">
        <p>{{Edad}}</p>
    </div>
    <div id="Telefono">
        <p>{{Telefono}}</p>
    </div>
    <div id="Descripcion">
        <p>{{DescripcionDoers}}</p>
    </div>
    <div id="Editar">
        <a href="/doers/editar/{{idDoers}}"><button>Editar</button></a>
    </div>
    {{/each}}
</div>