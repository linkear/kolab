<form action="/Proyecto/Lista/{{user.idUsuarios}}" method="get"></form>
<div id="contenido">
    <div class="row">
        {{#each listaProyecto}}
        <div id="trajeta">
            <div id="Nombre">
                <label>{{NombreProyecto}}</label>
            </div>
            <div id="Descripcion">
                <label>Descripci√≥n:</label>
